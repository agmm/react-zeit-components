{
  "name": "react-zeit-components",
  "version": "0.2.1",
  "description": "Zeit's design components to React",
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "webpack --watch --config webpack.dev.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "webpack --config webpack.prod.js",
    "postbuild": "node scripts/copy-files",
    "release": "npm run build && npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cube5/react-zeit-components.git"
  },
  "keywords": [
    "zeit",
    "zeit-design",
    "zeit-components",
    "react",
    "react-component"
  ],
  "author": "Daniel Sanchez <jdaniel.saed@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cube5/react-zeit-components/issues"
  },
  "homepage": "https://github.com/cube5/react-zeit-components#readme",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@storybook/addon-a11y": "^4.1.3",
    "@storybook/addon-actions": "^4.1.3",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/addon-knobs": "^4.1.3",
    "@storybook/addon-links": "^4.1.3",
    "@storybook/addons": "^4.1.3",
    "@storybook/react": "^4.1.3",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.4",
    "@types/webpack-env": "^1.13.6",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^1.0.0",
    "fs-extra": "^7.0.1",
    "jest": "^23.6.0",
    "react": "^16.7.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.7.0",
    "source-map-loader": "^0.2.4",
    "storybook": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "styled-components": "^4.1.3"
  }
